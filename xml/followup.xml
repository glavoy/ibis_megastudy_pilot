<?xml version = '1.0' encoding = 'utf-8'?>
<survey>


	<question type = 'automatic' fieldname = 'starttime' fieldtype = 'datetime'>
	</question>


	<question type = 'radio' fieldname = 'countrycode' fieldtype = 'integer'>
		<text>Select The country/region</text>
		<responses>
			<response value = '1'>Uganda</response>
			<response value = '2'>Kenya</response>
		</responses>
	</question>


	<question type = 'automatic' fieldname = 'subjid' fieldtype = 'text'>
		<maxCharacters>20</maxCharacters>
	</question>


	<question type = 'automatic' fieldname = 'participants_name' fieldtype = 'text'>
		<maxCharacters>80</maxCharacters>
	</question>


	<question type = 'automatic' fieldname = 'arm' fieldtype = 'integer'>
	</question>


	<question type = 'text' fieldname = 'tabletnum' fieldtype = 'text_integer'>
		<text>Please enter the tablet number of the tablet you are using</text>
		<maxCharacters>2</maxCharacters>
	</question>


	<question type = 'text' fieldname = 'interviewer_id' fieldtype = 'text_integer'>
		<text>Please enter the Interviewer ID that has been assigned to you</text>
		<maxCharacters>2</maxCharacters>
	</question>


	<question type = 'radio' fieldname = 'health_facility_ke' fieldtype = 'integer'>
		<text>Health facility name</text>
		<responses>
			<response value = '21'>Homa Bay Teaching and Referral Hospital</response>
			<response value = '22'>Rachuonyo District Hospital</response>
			<response value = '23'>Suba District Hospital</response>
			<response value = '24'>Ndhiwa District Hospital</response>
			<response value = '99'>Other</response>
		</responses>
		<preskip>
			<skip fieldname='countrycode' condition = '&lt;&gt;' response='2' response_type='fixed' skiptofieldname ='health_facility_ug'></skip>
		</preskip>
	</question>


	<question type = 'radio' fieldname = 'health_facility_ug' fieldtype = 'integer'>
		<text>Health facility name</text>
		<responses>
			<response value = '11'>Bushenyi HCIV</response>
			<response value = '12'>Ishaka Adventist Hospital (Bushenyi)</response>
			<response value = '13'>Ishongororo HCIV (Ibanda)</response>
			<response value = '14'>Ruhoko HCIV (Ibanda)</response>
			<response value = '99'>Other</response>
		</responses>
		<preskip>
			<skip fieldname='countrycode' condition = '&lt;&gt;' response='1' response_type='fixed' skiptofieldname ='health_facility'></skip>
		</preskip>
	</question>


	<question type = 'text' fieldname = 'health_facility_oth' fieldtype = 'text'>
		<text>Specify other health facility</text>
		<maxCharacters>80</maxCharacters>
		<preskip>
			<skip fieldname='health_facility_ke' condition = '&lt;&gt;' response='99' response_type='fixed' skiptofieldname ='health_facility'></skip>
		</preskip>
	</question>


	<question type = 'automatic' fieldname = 'health_facility' fieldtype = 'integer'>
	</question>


	<question type = 'radio' fieldname = 'hiv_testing_site' fieldtype = 'integer'>
		<text>HIV testing site</text>
		<responses>
			<response value = '1'>OPD</response>
			<response value = '2'>ANC/MCH</response>
			<response value = '3'>DICE/Key Pops</response>
			<response value = '99'>Other</response>
		</responses>
	</question>


	<question type = 'text' fieldname = 'hiv_testing_site_oth' fieldtype = 'text'>
		<text>Specify other HIV testing site</text>
		<maxCharacters>80</maxCharacters>
		<preskip>
			<skip fieldname='hiv_testing_site' condition = '&lt;&gt;' response='99' response_type='fixed' skiptofieldname ='hivtest_result_today'></skip>
		</preskip>
	</question>


	<question type = 'radio' fieldname = 'hivtest_result_today' fieldtype = 'integer'>
		<text>What was the participant’s HIV test result today?</text>
		<responses>
			<response value = '0'>Negative</response>
			<response value = '1'>Positive</response>
			<response value = '2'>Indeterminate</response>
		</responses>
		<postskip>
			<skip fieldname='hivtest_result_today' condition = '=' response='0' response_type='fixed' skiptofieldname ='hiv_risk_status'></skip>
			<skip fieldname='hivtest_result_today' condition = '=' response='1' response_type='fixed' skiptofieldname ='hiv_risk_status'></skip>
			<skip fieldname='hivtest_result_today' condition = '=' response='2' response_type='fixed' skiptofieldname ='hiv_risk_status'></skip>
		</postskip>
	</question>


	<question type = 'radio' fieldname = 'hiv_positive_referral' fieldtype = 'integer'>
		<text>If HIV result = positive, Was the participant referred for HIV care and ART?</text>
		<responses>
			<response value = '1'>Yes</response>
			<response value = '2'>No</response>
		</responses>
		<preskip>
			<skip fieldname='hivtest_result_today' condition = '&lt;&gt;' response='1' response_type='fixed' skiptofieldname ='prep_referral'></skip>
		</preskip>
	</question>


	<question type = 'radio' fieldname = 'prep_referral' fieldtype = 'integer'>
		<text>If HIV result = negative, Was the participant referred for PrEP or PEP?</text>
		<responses>
			<response value = '1'>Referred For PrEP</response>
			<response value = '2'>Referred for PEP</response>
			<response value = '3'>Currently Taking PrEP</response>
			<response value = '4'>Not on PrEP and no Referral</response>
		</responses>
		<preskip>
			<skip fieldname='hivtest_result_today' condition = '&lt;&gt;' response='0' response_type='fixed' skiptofieldname ='information_screen_interviewer'></skip>
		</preskip>
	</question>


	<question type = 'information' fieldname = 'infor_screen_1' fieldtype = 'n/a'>
		<text>READ TO RESPONDENT: Now my next questions are about HIV and AIDS, and for each one I’d like for you to tell me your answer.</text>
		<preskip>
			<skip fieldname='hivtest_result_today' condition = '&lt;&gt;' response='1' response_type='fixed' skiptofieldname ='infor_screen_2'></skip>
		</preskip>
	</question>


	<question type = 'radio' fieldname = 'hiv_risk_status' fieldtype = 'integer'>
		<text>Do you think your chances of getting HIV/AIDS in the coming year are high, moderate, low, or no risk at all?</text>
		<responses>
			<response value = '1'>High</response>
			<response value = '2'>Moderate</response>
			<response value = '3'>Low</response>
			<response value = '4'>No Risk at all</response>
		</responses>
		<dont_know>-7</dont_know>
		<refuse>-8</refuse>
	</question>


	<question type = 'radio' fieldname = 'knows_art' fieldtype = 'integer'>
		<text>Have you heard about antiretroviral therapy (ART), the medicines for treating HIV?</text>
		<responses>
			<response value = '1'>Yes</response>
			<response value = '2'>No</response>
		</responses>
		<dont_know>-7</dont_know>
		<refuse>-8</refuse>
	</question>


	<question type = 'radio' fieldname = 'knows_prep' fieldtype = 'integer'>
		<text>Have you heard about PrEP (Pre-exposure prophylaxis): an HIV medication that is taken to prevent people from getting HIV?</text>
		<responses>
			<response value = '1'>Yes</response>
			<response value = '2'>No</response>
		</responses>
		<dont_know>-7</dont_know>
		<refuse>-8</refuse>
	</question>


	<question type = 'radio' fieldname = 'on_prep' fieldtype = 'integer'>
		<text>Are you currently taking PrEP (HIV medication to prevent you from getting HIV)? This is usually a pill taken daily.</text>
		<responses>
			<response value = '1'>Yes</response>
			<response value = '2'>No</response>
		</responses>
		<dont_know>-7</dont_know>
		<refuse>-8</refuse>
	</question>


	<question type = 'radio' fieldname = 'ready_for_hivtest' fieldtype = 'integer'>
		<text>
			How likely are you to go for an HIV test again sometime in the next 6 months?
			[READ ALL ANSWER CHOICES]
		</text>
		<responses>
			<response value = '1'>Very Likely</response>
			<response value = '2'>Somewhat Likely</response>
			<response value = '3'>Not Sure</response>
			<response value = '4'>Unlikely</response>
			<response value = '5'>Very Unlikely</response>
		</responses>
		<refuse>-8</refuse>
	</question>


	<question type = 'radio' fieldname = 'motivation' fieldtype = 'integer'>
		<text>
			ASK ONLY FOR THOSE WHO DON’T REPORT “VERY UNLIKELY”
			What do you think your main motivation might be for getting another HIV test in the future?
		</text>
		<responses>
			<response value = '1'>Staying Healthy/HIV-Negative</response>
			<response value = '2'>Preventing my sexual partners from becoming HIV infected</response>
			<response value = '3'>Fearful of Getting Sick</response>
			<response value = '99'>Other (Specify)</response>
		</responses>
		<preskip>
			<skip fieldname='motivation' condition = '&lt;&gt;' response='5' response_type='fixed' skiptofieldname ='infor_screen_2'></skip>
			<skip fieldname='motivation' condition = '&lt;&gt;' response='-7' response_type='fixed' skiptofieldname ='infor_screen_2'></skip>
			<skip fieldname='motivation' condition = '&lt;&gt;' response='-8' response_type='fixed' skiptofieldname ='infor_screen_2'></skip>
		</preskip>
		<dont_know>-7</dont_know>
		<refuse>-8</refuse>
	</question>


	<question type = 'text' fieldname = 'motivation_oth' fieldtype = 'text'>
		<text>Specify the other motivation reason for getting another HIV test in the future</text>
		<maxCharacters>255</maxCharacters>
		<preskip>
			<skip fieldname='motivation' condition = '&lt;&gt;' response='99' response_type='fixed' skiptofieldname ='infor_screen_2'></skip>
		</preskip>
	</question>


	<question type = 'information' fieldname = 'infor_screen_2' fieldtype = 'n/a'>
		<text>[Interviewer]: Now I’d like to ask you a few questions about your recent sexual history (since you last tested for HIV).</text>
	</question>


	<question type = 'radio' fieldname = 'multiple_partners' fieldtype = 'integer'>
		<text>Have you had more than one sexual partner since your last HIV test?</text>
		<responses>
			<response value = '1'>Yes</response>
			<response value = '2'>No</response>
		</responses>
	</question>


	<question type = 'radio' fieldname = 'new_partner' fieldtype = 'integer'>
		<text>Have you had a new sexual partner since your last HIV test?</text>
		<responses>
			<response value = '1'>Yes</response>
			<response value = '2'>No</response>
		</responses>
	</question>


	<question type = 'radio' fieldname = 'unprotected_sex' fieldtype = 'integer'>
		<text>Have you had sex without a condom since your last HIV test?</text>
		<responses>
			<response value = '1'>Yes</response>
			<response value = '2'>No</response>
		</responses>
	</question>


	<question type = 'radio' fieldname = 'hiv_positive_partner' fieldtype = 'integer'>
		<text>Since your last HIV test, have you had a sexual partner whom you know is HIV-positive?</text>
		<responses>
			<response value = '1'>Yes</response>
			<response value = '2'>No</response>
		</responses>
	</question>


	<question type = 'radio' fieldname = 'sti_history' fieldtype = 'integer'>
		<text>Since your last HIV test, have you been diagnosed with, treated for, or had symptoms of a sexually transmitted infection?</text>
		<responses>
			<response value = '1'>Yes</response>
			<response value = '2'>No</response>
		</responses>
	</question>


	<question type = 'radio' fieldname = 'sex_for_compensation' fieldtype = 'integer'>
		<text>Have you received any compensation or gifts in exchange for sex since your last HIV test?</text>
		<responses>
			<response value = '1'>Yes</response>
			<response value = '2'>No</response>
		</responses>
	</question>


	<question type = 'radio' fieldname = 'paid_for_sex' fieldtype = 'integer'>
		<text>Have you paid or given any compensation or gifts in exchange for sex since your last HIV test?</text>
		<responses>
			<response value = '1'>Yes</response>
			<response value = '2'>No</response>
		</responses>
	</question>


	<question type = 'radio' fieldname = 'dice_clinic' fieldtype = 'integer'>
		<text>Have you been seen in a DICE clinic since your last HIV test (i.e. are you PWID, MSM, sex worker, transgender)?</text>
		<responses>
			<response value = '1'>Yes</response>
			<response value = '2'>No</response>
		</responses>
	</question>


	<question type = 'radio' fieldname = 'on_pep' fieldtype = 'integer'>
		<text>Have you recently initiated PEP (Post-exposure prophylaxis) since your last HIV test, or have you been deemed eligible for PEP at this visit?</text>
		<responses>
			<response value = '1'>Yes</response>
			<response value = '2'>No</response>
		</responses>
	</question>


	<question type = 'radio' fieldname = 'recent_hiv_exposure' fieldtype = 'integer'>
		<text>Since your last HIV test, have you had a recent HIV exposure risk (for example, had a condom break, raped someone, been raped)?</text>
		<responses>
			<response value = '1'>Yes</response>
			<response value = '2'>No</response>
		</responses>
	</question>


	<question type = 'information' fieldname = 'information_screen_ra1' fieldtype = 'n/a'>
		<text>Questions for RA/counselor about referrals</text>
	</question>


	<question type = 'radio' fieldname = 'hiv_positive_referral_accept' fieldtype = 'integer'>
		<text>[If HIV result = positive] Did this participant accept referral for HIV care and ART?</text>
		<responses>
			<response value = '1'>Yes</response>
			<response value = '2'>No</response>
		</responses>
		<preskip>
			<skip fieldname='hivtest_result_today' condition = '&lt;&gt;' response='1' response_type='fixed' skiptofieldname ='prep_pep_eligible'></skip>
		</preskip>
	</question>


	<question type = 'radio' fieldname = 'prep_pep_eligible' fieldtype = 'integer'>
		<text>[IF HIV result = negative AND not currently taking PrEP (Q5)] Was the participant eligible for PrEP or PEP?</text>
		<responses>
			<response value = '1'>Yes</response>
			<response value = '2'>No</response>
		</responses>
		<preskip>
			<skip fieldname='on_prep' condition = '&lt;&gt;' response='2' response_type='fixed' skiptofieldname ='information_screen_interviewer'></skip>
			<skip fieldname='hivtest_result_today' condition = '&lt;&gt;' response='0' response_type='fixed' skiptofieldname ='information_screen_interviewer'></skip>
		</preskip>
	</question>


	<question type = 'radio' fieldname = 'prep_pep_referral_accept' fieldtype = 'integer'>
		<text>[If PrEP/PEP eligible = yes] Did this participant accept referral for PrEP or PEP</text>
		<responses>
			<response value = '1'>Yes</response>
			<response value = '2'>No</response>
		</responses>
	</question>


	<question type = 'information' fieldname = 'information_screen_interviewer' fieldtype = 'n/a'>
		<text>Please answer a few questions about this survey experience.</text>
	</question>


	<question type = 'text' fieldname = 'comments' fieldtype = 'text'>
		<text>Use this field to record any special comments/info/problems</text>
		<maxCharacters>255</maxCharacters>
		<na>-6</na>
	</question>


	<question type = 'text' fieldname = 'subjid2' fieldtype = 'text'>
		<text>Re-write Study ID</text>
		<maxCharacters>20</maxCharacters>
		<logic_check>
			<logic fieldname='subjid2' condition = '&lt;&gt;' response = 'subjid' response_type = 'dynamic' condition2 = '=' currentresponse = '1' message = 'This does not match your previous entry!'></logic>
		</logic_check>
	</question>


	<question type = 'automatic' fieldname = 'uniqueid' fieldtype = 'text'>
	</question>


	<question type = 'automatic' fieldname = 'swver' fieldtype = 'text'>
	</question>


	<question type = 'automatic' fieldname = 'stoptime' fieldtype = 'datetime'>
	</question>


	<question type = 'information' fieldname = 'end_of_questions' fieldtype = 'n/a'>
		<text>Press the 'Next' button to save the data.</text >
	</question>


</survey>
